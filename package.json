{
  "name": "ControlCast",
  "version": "0.4.0",
  "description": "ControlCast - A Launchpad Controller desktop application for streamers",
  "main": "./app/index.js",
  "scripts": {
    "start": "electron ./app",
    "build": "electron-builder --win portable",
    "rebuild": "electron-rebuild"
  },
  "author": "DBKynd",
  "devDependencies": {
    "electron": "^28.3.3",
    "electron-builder": "^24.13.3",
    "electron-rebuild": "^3.2.9"
  },
  "dependencies": {
    "electron-config": "^2.0.0",
    "express": "^4.18.3",
    "jquery": "^3.7.1",
    "keycode": "^2.2.1",
    "launchpadder": "^0.0.5",
    "@nut-tree-fork/nut-js": "^4.2.0",
    "midi": "^2.0.0",
    "moment": "^2.30.1",
    "noty": "^3.2.0-beta",
    "request": "^2.88.2",
    "simpleflakes": "^1.2.3",
    "socket.io": "^4.7.5",
    "superagent": "^8.1.2",
    "underscore": "^1.13.6",
    "usb-detection": "^4.14.2",
    "winston": "^3.13.0",
    "winston-loggly-bulk": "^1.4.2",
    "woopra": "^1.0.2"
  },
  "build": {
    "appId": "com.controlcast.app",
    "productName": "ControlCast",
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": "portable"
    },
    "npmRebuild": false
  }
}